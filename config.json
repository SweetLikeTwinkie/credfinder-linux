{
  "scan_paths": {
    "ssh": [
      "~/.ssh/",
      "/etc/ssh/",
      "/home/*/.ssh/",
      "/root/.ssh/"
    ],
    "browsers": {
      "chrome": [
        "~/.config/google-chrome/Default/",
        "~/.config/chromium/Default/",
        "~/.config/brave-browser/Default/",
        "/home/*/.config/google-chrome/Default/",
        "/home/*/.config/chromium/Default/",
        "/home/*/.config/brave-browser/Default/"
      ],
      "firefox": [
        "~/.mozilla/firefox/*.default*/",
        "~/.mozilla/firefox/*.default-release/",
        "/home/*/.mozilla/firefox/*.default*/",
        "/home/*/.mozilla/firefox/*.default-release/"
      ]
    },
    "config_files": [
      "~/.bash_history",
      "~/.zsh_history",
      "~/.git-credentials",
      "~/.npmrc",
      "~/.pypirc",
      "~/.netrc",
      "~/.aws/credentials",
      "~/.aws/config",
      "~/.config/gcloud/credentials.db",
      "~/.azure/credentials",
      "~/.docker/config.json",
      "~/.kube/config",
      "/home/*/.bash_history",
      "/home/*/.zsh_history",
      "/home/*/.git-credentials",
      "/home/*/.npmrc",
      "/home/*/.pypirc",
      "/home/*/.netrc",
      "/home/*/.aws/credentials",
      "/home/*/.aws/config",
      "/home/*/.docker/config.json",
      "/home/*/.kube/config"
    ],
    "common_files": [
      "~/Downloads/*.env*",
      "~/Downloads/*.config*",
      "~/Desktop/*.env*"
    ],
    "history_files": [
      "~/.bash_history",
      "~/.zsh_history", 
      "~/.sh_history",
      "~/.history"
    ],
    "env_file_patterns": [
      "~/.env*",
      "~/.config/*/.env*",
      "~/.local/share/*/.env*",
      "~/projects/*/.env*",
      "~/workspace/*/.env*"
    ],
    "git_config_paths": [
      "~/.git-credentials",
      "~/.gitconfig",
      "~/.config/git/config"
    ],
    "git": [
      "~/",
      "~/projects/",
      "~/work/", 
      "~/workspace/",
      "~/Documents/",
      "~/Desktop/",
      "~/code/",
      "~/dev/"
    ]
  },
  "exclusions": {
    "directories": [
      ".venv",
      "venv", 
      "node_modules",
      "__pycache__",
      ".pytest_cache",
      ".mypy_cache",
      ".tox",
      ".egg-info",
      ".eggs",
      "site-packages",
      ".npm",
      ".yarn",
      ".pnpm-store",
      ".cache",
      ".local/share/Trash",
      ".Trash",
      "trash",
      ".DS_Store",
      ".idea",
      ".vscode",
      ".gradle",
      ".m2"
    ],
    "file_patterns": [
      "*.pyc",
      "*.pyo", 
      "*.so",
      "*.dll",
      "*.exe",
      "*.bin",
      "*.obj",
      "*.o",
      "*.a",
      "*.lib",
      "*.jar",
      "*.war",
      "*.class",
      "*.min.js",
      "*.min.css",
      "*.map",
      "*.lock",
      "package-lock.json",
      "yarn.lock",
      "Pipfile.lock",
      "poetry.lock",
      "*.whl",
      "*.tar.gz",
      "*.zip",
      "*.rar",
      "*.7z",
      "*.iso",
      "*.img",
      "*.dmg",
      "*.log",
      "*.out",
      "*test*",
      "*example*",
      "*sample*",
      "*demo*",
      "*.test.*",
      "test_*",
      "*_test.*"
    ],
    "path_patterns": [
      "*/.local/share/Trash/*",
      "*/.Trash/*", 
      "*/trash/*",
      "*/.venv/*",
      "*/venv/*",
      "*/node_modules/*",
      "*/__pycache__/*",
      "*/site-packages/*",
      "*/.cache/*",
      "*/build/lib/*",
      "*/build/bdist*",
      "*/dist/build/*",
      "*/.tox/*",
      "*/.pytest_cache/*",
      "*/.mypy_cache/*"
    ]
  },
  "patterns": {
    "aws_keys": [
      "AKIA(?!EXAMPLE)[0-9A-Z]{16}",
      "aws_access_key_id\\s*[=:]\\s*(?!EXAMPLE)[A-Z0-9]{20}",
      "aws_secret_access_key\\s*[=:]\\s*(?!EXAMPLE)[A-Za-z0-9/+=]{40}"
    ],
    "api_tokens": [
      "sk_live_[a-zA-Z0-9]{24,}",
      "sk_test_[a-zA-Z0-9]{24,}",
      "pk_live_[a-zA-Z0-9]{24,}",
      "pk_test_[a-zA-Z0-9]{24,}",
      "ghp_[a-zA-Z0-9]{36}",
      "gho_[a-zA-Z0-9]{36}",
      "ghu_[a-zA-Z0-9]{36}",
      "ghs_[a-zA-Z0-9]{36}",
      "ghr_[a-zA-Z0-9]{36}",
      "xoxb-[0-9]{12}-[0-9]{12}-[a-zA-Z0-9]{24}",
      "xoxp-[0-9]{12}-[0-9]{12}-[a-zA-Z0-9]{24}",
      "discord.*[a-zA-Z0-9]{24}\\.[a-zA-Z0-9]{6}\\.[a-zA-Z0-9_\\-]{27}",
      "AIza[0-9A-Za-z\\-_]{35}",
      "ya29\\.[a-zA-Z0-9_-]{50,}",
      "1//[a-zA-Z0-9_-]{40,}"
    ],
    "environment_vars": [
      "export\\s+[A-Z_]*(?:PASSWORD|SECRET|KEY|TOKEN)[A-Z_]*=[\"']?[^\\s\"']{8,}[\"']?",
      "[A-Z_]*(?:PASSWORD|SECRET|KEY|TOKEN)[A-Z_]*=[\"']?[^\\s\"']{8,}[\"']?",
      "\\$\\{[A-Z_]*(?:PASSWORD|SECRET|KEY|TOKEN)[A-Z_]*\\}"
    ],

    "jwt_tokens": [
      "eyJ[A-Za-z0-9-_=]{20,}\\.[A-Za-z0-9-_=]{20,}\\.[A-Za-z0-9-_.+/=]{20,}"
    ],
    "passwords": [
      "password\\s*[=:]\\s*[\"'](?!test|example|demo|foobar|changeme|secret123|password123|placeholder)[^\"']{8,}[\"']",
      "passwd\\s*[=:]\\s*[\"'](?!test|example|demo|foobar|changeme|secret123|password123)[^\"']{8,}[\"']",
      "secret\\s*[=:]\\s*[\"'](?!test|example|demo|foobar|changeme|secret123)[^\"']{12,}[\"']",
      "api_key\\s*[=:]\\s*[\"'](?!test|example|demo|your-key|put-your)[^\"']{16,}[\"']",
      "access_token\\s*[=:]\\s*[\"'](?!test|example|demo|your-token)[^\"']{16,}[\"']",
      "auth_token\\s*[=:]\\s*[\"'](?!test|example|demo|your-token)[^\"']{16,}[\"']"
    ],
    "database_urls": [
      "mysql://[^\\s\"']+",
      "postgresql://[^\\s\"']+",
      "mongodb://[^\\s\"']+",
      "redis://[^\\s\"']+",
      "sqlite://[^\\s\"']+"
    ],
    "private_keys": [
      "-----BEGIN (RSA |DSA |EC |OPENSSH )?PRIVATE KEY-----",
      "-----BEGIN PGP PRIVATE KEY BLOCK-----"
    ],
    "credentials": [
      "GITHUB_TOKEN[=:\\s]+[\"']?[a-zA-Z0-9_]{20,}[\"']?",
      "SLACK_TOKEN[=:\\s]+[\"']?[a-zA-Z0-9_-]{20,}[\"']?",
      "TWITTER_[A-Z_]*[=:\\s]+[\"']?[a-zA-Z0-9_]{15,}[\"']?",
      "FACEBOOK_[A-Z_]*[=:\\s]+[\"']?[a-zA-Z0-9_]{15,}[\"']?",
      "GOOGLE_[A-Z_]*[=:\\s]+[\"']?[a-zA-Z0-9_-]{20,}[\"']?",
      "DATABASE_URL[=:\\s]+[\"']?(mysql|postgresql|mongodb|redis)://[^\\s\"']{10,}[\"']?"
    ]
  },
  "module_settings": {
    "ssh": {
      "key_patterns": [
        "id_rsa*", "id_dsa*", "id_ecdsa*", "id_ed25519*",
        "id_xmss*", "ssh_host_*_key"
      ],
      "secure_permissions": [384, 256],
      "key_data_truncate_length": 50,
      "ssh_agent_timeout": 5,
      "fingerprint_timeout": 5
    },
    "browser": {
      "chrome_default_password": "peanuts",
      "chrome_salt": "saltysalt",
      "chrome_key_length": 16,
      "chrome_iterations": 1,
      "chrome_iv_length": 16,
      "chrome_version_prefix_length": 3,
      "secret_tool_timeout": 5,
      "kwallet_timeout": 5,
      "database_files": {
        "chrome_passwords": "Login Data",
        "chrome_cookies": "Cookies", 
        "chrome_autofill": "Web Data",
        "firefox_key_db": "key4.db",
        "firefox_logins": "logins.json"
      },
      "secret_storage_labels": [
        "Chrome Safe Storage",
        "Chromium Safe Storage"
      ],
      "max_copy_retries": 3
    },
    "dotfile": {
      "max_file_size_bytes": 1048576,
      "content_preview_length": 100,
      "context_size": 25
    },
    "file_grep": {
      "file_extensions": [
        "*.env", "*.ini", "*.conf", "*.config", 
        "*.yaml", "*.yml", "*.properties"
      ],
      "search_base_paths": [
        "~/.config/", "~/.local/share/", "~/projects/", "~/workspace/", "~/work/", "~/code/", "~/dev/"
      ],
      "max_files_per_pattern": 50,
      "max_files_common_location": 20,
      "max_file_size_bytes": 1048576,
      "max_file_read_bytes": 50000,
      "max_common_file_read_bytes": 25000,
      "process_scan_timeout": 10,
      "command_preview_length": 100,
      "context_size": 40,
      "min_match_length": 12,
      "binary_detection_chunk_size": 512
    },
    "history": {
      "max_file_size_bytes": 5242880,
      "context_size": 20,
      "risk_patterns": {
        "critical": ["aws_keys", "api_tokens", "jwt_tokens"],
        "medium": ["passwords"],
        "low": []
      }
    },
    "keyring": {
      "version_check_timeout": 5,
      "keyring_access_timeout": 30,
      "wallet_access_timeout": 15,
      "max_wallet_name_length": 64,
      "common_services": [
        "chrome", "chromium", "firefox", "brave",
        "ssh", "git", "aws", "docker", "kubernetes",
        "mysql", "postgresql", "redis", "mongodb",
        "vpn", "wifi", "network", "email", "imap", "smtp",
        "ftp", "sftp", "webdav", "dropbox", "onedrive", "google-drive"
      ]
    },
    "git": {
      "max_commits_scan": 25,
      "max_file_size_bytes": 1048576,
      "git_command_timeout": 10,
      "context_size": 50,
      "skip_binary_files": true,
      "sensitive_file_patterns": [
        "*.env*", "*.key", "*.pem", "*.p12", "*.pfx", "*.jks", 
        "*config*", "*secret*", "*credential*", "*password*",
        "*.yaml", "*.yml", "*.conf", "*.ini"
      ],
      "url_credential_patterns": [
        "://[^/\\s]+:[^/\\s@]+@",
        "^https?://[^/\\s]*:[^/\\s@]*@[^/\\s]+",
        "^ssh://[^/\\s]*:[^/\\s@]*@[^/\\s]+",
        "^ftp://[^/\\s]*:[^/\\s@]*@[^/\\s]+",
        "^mysql://[^/\\s]*:[^/\\s@]*@[^/\\s]+",
        "^postgresql://[^/\\s]*:[^/\\s@]*@[^/\\s]+",
        "^mongodb://[^/\\s]*:[^/\\s@]*@[^/\\s]+",
        "^redis://[^/\\s]*:[^/\\s@]*@[^/\\s]+"
      ],
      "performance_settings": {
        "max_concurrent_repos": 5,
        "commit_scan_timeout": 10,
        "file_scan_timeout": 5,
        "max_commit_diff_lines": 1000
      }
    }
  },

  "opsec": {
    "minimal_logging": false,
    "no_network_calls": true,
    "clean_exit": true,
    "file_permissions": "600",
    "log_to_file": false,
    "delete_temp_files": true
  },
  "reporting": {
    "false_positive_filters": {
      "programming_patterns": [
        "key=self.", "password=self.", "token=self.",
        "key=args.", "password=args.", "token=args.",
        "password=password", "key=key", "token=token",
        "aeskey=self.", "publickey=", "privatekey=",
        "receiver_key=", "sender_key=", "public_key=",
        "private_key=", "key_params=", "key_size=",
        "keylen=", "keyfile=", "key_file=", "key_password=",
        "key_id=", "key_identifier=", "key_cert=",
        "unsafe_skip_rsa_key_validation=",
        "enforce_key_length=", "key_agreement=",
        "key_encipherment=", "path_keyword=",
        "connection_keyword=", "sort_keys=", "skip_keys=",
        "tokentype=", "as_keyword=", "keyfunc=",
        "key=itemgetter", "key=operator", "key=sort",
        "key=backend", "key=parse_version", "key=str.",
        "key=get_", "key=self._", "key=functools",
        "key=device", "password={password!r}",
        "keyring=", "keychain=", "key=value",
        "password=self.password)", "key=binascii",
        "%s", "{0}", "{1}", "{2}", "{3}", "{4}",
        "secret = \"%s", "password = \"%s", "key = \"%s",
        ".format(", "% (", "hexlify(", ".decode(",
        "extrasecret", "printname", "secretitem"
      ],
      "dotnet_patterns": [
        "publickeytoken=", "publickey=", "msft_",
        "culture=neutral", "processorarchitecture=",
        "version=", ".dll", ".exe", "microsoft",
        "system.", "newtonsoft", "protobuf"
      ],
      "comment_patterns": [
        "# -*-", "# publickey/", "# written in",
        "written in 200", "signature primitive",
        "collection of python", "copyright",
        "all rights reserved", "mit license",
        "gpl license", "apache license", ":parameters:",
        "parameters:", "- key :", "object", "if a private"
      ],
      "test_patterns": [
        "example", "test", "demo", "sample", "placeholder",
        "foobar", "changeme", "secret123", "password123",
        "akiaiosfodnn7example", "lorem ipsum"
      ],
      "path_patterns": [
        ".py,sha256=", ".whl", "/site-packages/",
        "requirements.txt", "setup.py", "github.com",
        "localhost", "circleci.com", "127.0.0.1",
        "vendor/", ".travis.yml", "appveyor.yml",
        "/vendor/github.com/", "go.mod", "go.sum",
        ".git/", "node_modules/", "__pycache__/",
        "credentials.go", "credentials_info.go",
        "config/error.go", "config/hosts.go", "config/krb5conf.go",
        ".travis.yml", "appveyor.yml", "azure-pipelines.yml",
        "circle.yml", ".circleci/", ".github/workflows/"
      ],
      "git_diff_patterns": [
        "\")\n+", "\")\n-", "- +", "+ -", "\") +", "\") -",
        "try:", "except:", "if ", "else:", "logging.",
        "getpass(\"", "print()", "sys.exit", "\\n+", "\\n-"
      ],
      "config_regex_patterns": [
        "[^\\s", "^\\s", "\"+", "], \"",
        "\"mysql://", "\"postgresql://", "\"mongodb://",
        "\"redis://", "\"sqlite://", "database_urls",
        "private_keys", "\"-----begin", "url_credential_patterns"
      ],
      "documentation_patterns": [
        "| database connection strings |", "| ssh/pgp private keys |",
        "`mysql://", "`postgresql://", "- get-", "- set-",
        ".md'", "privesc/", "recon/", "exfiltration/",
        "docs/", "config_wiki", "architecture"
      ],
      "env_documentation_patterns": [
        "${tqdm_", "${github_", "${circle_", "default:",
        "[default:", "uses `${", "and `${", "token +",
        "token -", "channel_id", "chat_id"
      ],
      "library_api_patterns": [
        "credentials.go", "credentials_info.go", "ccache.go",
        "secretsdump.py", "mimikatz", "impacket/examples/",
        "/librairies/", "/modules/", "Get-GPPPassword",
        "Inject-LogonCredentials", "Get-SiteListPassword",
        "sekurlsa/modules", "powersploit", "bloodhound"
      ],
      "vendor_dependency_patterns": [
        "vendor/", "/vendor/github.com/", "/vendor/gopkg.in/",
        "node_modules/", "site-packages/", "__pycache__/",
        "go-uuid", "go-logging", "gokrb5", "spf13", "cobra",
        "pflag", "jcmturner", "dnsutils", "jessevdk", "go-flags"
      ]
    },
    "risk_mappings": {
      "severity_mapping": {
        "critical": "critical",
        "high": "critical", 
        "medium": "warning",
        "low": "info"
      },
      "risk_scores": {
        "critical": 9,
        "high": 7,
        "medium": 5,
        "low": 3
      }
    },
    "finding_categories": {
      "ssh_key_credential": "authentication_credential",
      "git_commit_credential": "version_control_credential",
      "git_config_credential": "configuration_credential",
      "git_remote_credential": "url_credential",
      "git_sensitive_file": "file_credential",
      "git_deleted_credential": "historical_credential",
      "browser_credential": "application_credential",
      "file_pattern_match": "potential_credential",
      "command_history_credential": "exposed_credential",
      "dotfile_credential": "configuration_credential"
    },
    "remediation_messages": {
      "git_commit_credential": "Remove credentials from git history and rotate exposed secrets",
      "git_config_credential": "Remove credentials from git config and use credential helpers",
      "git_remote_credential": "Use SSH keys or personal access tokens instead of embedded credentials",
      "git_sensitive_file": "Remove sensitive files from git history and add to .gitignore",
      "git_deleted_credential": "Verify credentials are rotated if they were exposed before deletion",
      "ssh_key_credential": "Secure SSH keys with proper permissions and passphrases",
      "browser_credential": "Use browser's built-in password manager securely",
      "file_pattern_match": "Review file contents and secure any credentials found",
      "command_history_credential": "Remove sensitive commands from history and use environment variables",
      "dotfile_credential": "Move credentials to secure configuration management"
    }
  },
  "output": {
    "formats": ["json", "html", "console"],
    "default_format": "json",
    "output_dir": "./reports",
    "compress_results": false,
    "include_metadata": true,
    "include_raw_data": false,
    "include_execution_data": false,
    "json_indent": 2,
    "timestamp_format": "%Y%m%d_%H%M%S"
  },
  "modules": {
    "ssh": {
      "enabled": true,
      "priority": 1,
      "timeout": 30,
      "parallel_safe": true,
      "requires_privileges": false,
      "estimated_time": "fast",
      "resource_intensive": false,
      "dependencies": []
    },
    "dotfiles": {
      "enabled": true,
      "priority": 2,
      "timeout": 60,
      "parallel_safe": true,
      "requires_privileges": false,
      "estimated_time": "fast",
      "resource_intensive": false,
      "dependencies": []
    },
    "history": {
      "enabled": true,
      "priority": 3,
      "timeout": 30,
      "parallel_safe": true,
      "requires_privileges": false,
      "estimated_time": "fast",
      "resource_intensive": false,
      "dependencies": []
    },
    "browser": {
      "enabled": true,
      "priority": 4,
      "timeout": 120,
      "parallel_safe": false,
      "requires_privileges": false,
      "estimated_time": "medium",
      "resource_intensive": false,
      "dependencies": []
    },
    "keyring": {
      "enabled": true,
      "priority": 5,
      "timeout": 60,
      "parallel_safe": false,
      "requires_privileges": false,
      "estimated_time": "medium",
      "resource_intensive": false,
      "dependencies": []
    },
    "file_grep": {
      "enabled": true,
      "priority": 6,
      "timeout": 90,
      "parallel_safe": true,
      "requires_privileges": false,
      "estimated_time": "medium",
      "resource_intensive": false,
      "dependencies": []
    },
    "git": {
      "enabled": true,
      "priority": 2,
      "timeout": 120,
      "parallel_safe": true,
      "requires_privileges": false,
      "estimated_time": "fast",
      "resource_intensive": false,
      "dependencies": ["git"]
    }
  },
  "logging": {
    "level": "INFO",
    "format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s",
    "console_logging": true,
    "use_colors": true
  }
} 